        "vue-naver-maps": "^0.12.1",
        "vue-router": "^3.0.7",
        "vue-session": "^1.0.0",
        "vue-simple-alert": "^1.1.1",
        "vuetify": "^2.0.0",
        "vuex": "^3.1.1",
        "vuex-persistedstate": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.10.0",
        "@vue/cli-plugin-eslint": "^3.10.0",
        "@vue/cli-service": "^3.10.0",
        "babel-eslint": "^10.0.1",
